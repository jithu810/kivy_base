#:import Window kivy.core.window.Window


<Home>:
    name: 'home'
    FloatLayout:
        size_hint:(1,1)
        
        


        MDFillRoundFlatButton:
            text: "main page"
            custom_color: 0, 1, 0, 1
            text_color: 0, 1, 0, 1
            line_color: 0, 0, 1, 1
            size_hint:.1,.05
            pos_hint:{'x':.01,'y':.90}
            on_press:
                app.click_sound
                app.progress()
                app.switch_screen('mainpage')



    


           
    
        
            

            

   
           

                

               
                  


